if(NOT DEFINED XP_DEV_@PRJ@_DIST)
  set(XP_DEV_@PRJ@_DIST ${CMAKE_CURRENT_LIST_DIR})
endif()
set(XP_DEV_@PRJ@_DIST ${XP_DEV_@PRJ@_DIST} CACHE PATH
  "developer-specified @NAME@ dist directory" FORCE
  )
option(XP_DEV_@PRJ@ "build with developer-specified @NAME@ dist directory" OFF)
if(XP_DEV_@PRJ@)
  set_property(CACHE XP_DEV_@PRJ@_DIST PROPERTY TYPE PATH)
  set(tgtsFile ${XP_DEV_@PRJ@_DIST}/share/cmake/@NAME@-targets.cmake)
  set(@PRJ@_VER "[${XP_DEV_@PRJ@_DIST}]")
else()
  set_property(CACHE XP_DEV_@PRJ@_DIST PROPERTY TYPE INTERNAL)
  set(tgtsFile ${CMAKE_CURRENT_LIST_DIR}/tgt-@NAME@/@NAME@-targets.cmake)
  set(@PRJ@_VER "@VER@ [@PROJECT_NAME@]")
endif()
set(reqVars @PRJ@_VER)
@FIND_DEPS@include(${tgtsFile})
@USE_VARS@include(FindPackageHandleStandardArgs)
set(FPHSA_NAME_MISMATCHED TRUE) # find_package_handle_standard_args NAME_MISMATCHED (prefix usexp-)
find_package_handle_standard_args(@NAME@ REQUIRED_VARS ${reqVars})
mark_as_advanced(${reqVars})
